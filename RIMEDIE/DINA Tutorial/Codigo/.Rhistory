d2a <- CDM::din( data=sim.dina, q.matrix=sim.qmatrix,
guess.equal=TRUE, slip.equal=FALSE )
d2a$coef
# Equal guessing and slipping parameters
d2b <- CDM::din( data=sim.dina, q.matrix=sim.qmatrix,
guess.equal=TRUE, slip.equal=TRUE )
d2b$coef
