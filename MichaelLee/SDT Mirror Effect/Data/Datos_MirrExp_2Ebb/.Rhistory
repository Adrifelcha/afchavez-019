#layout(matrix(1:2,ncol=1))
for(archive in dir()){
jaime <- read.csv(archive)
fa <- NULL
hits <- NULL
conf <- NULL
for(nce in sort(unique(jaime$tipo))){
fa <- append(fa, sum(jaime$Falsas.alarmas[jaime$tipo==nce]=='True'))
