    thetah_A[i] ~ dnorm(muHA,sdHA)
    thetaf_A[i] ~ dnorm(muFA,sdFA)
    thetah_B[i] ~ dnorm(muHB,sdHB)
    thetaf_B[i] ~ dnorm(muFB,sdFB)
